---
import Layout from '~/layouts/PageLayout.astro';

import Header from '~/components/widgets/Header.astro';
import Hero2 from '~/components/widgets/Hero2.astro';
import CallToAction from '~/components/widgets/CallToAction.astro';

const meta = {
  title: "Coming Soon",
};
---

<Layout {meta}>
  <Fragment slot="announcement"></Fragment>
  <Fragment slot="header"></Fragment>
  <Hero2
    callToAction={{ text: 'Contact', href: 'mailto:anniecardinalcoaching@gmail.com', icon: 'tabler:send' }}
    image={{ src: import('~/assets/images/hula-hoop.jpg'), alt: 'Annie Cardinal Coaching' }}
  >
    <Fragment slot="title">Annie Cardinal Coaching</Fragment>
    <Fragment slot="subtitle">
      Thank you for your interest in money coaching services from Annie Cardinal Coaching!
      I look forward to working together to empower you to reach your financial goals.
    </Fragment>
  </Hero2>
  <Fragment slot="footer"></Fragment>
</Layout>
